local c = {
	Players = game:GetService("Players"),
	Workspace = game:GetService("Workspace"),
	LocalPlayer = game:GetService("Players").LocalPlayer,
	destroying = false,
	whitelist = {
		["Lucaas035"] = true,
		["scary_games7"] = true,
		["PandaRose010"] = true,
		["chinarelli"] = true,
		["uhinubjk"] = true,
		["Leandro7162w"] = true,
		["medahighrankbliz"] = true,
		["yukki_farofa0linda"] = true,
		["Madarautrira1235m"] = true,
